{
  "parser": "babel-eslint",
  "extends": [
    "defaults"
  ],
  "rules": {
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "semi": [2, "always"],
    "no-console": 1
  },
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true
  },
  "globals": {
    "sinon": true
  }
}
